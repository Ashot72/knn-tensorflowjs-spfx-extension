{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "knn-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/knn/KnnCommandSet.js",
          "manifest": "./src/extensions/knn/KnnCommandSet.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "tf": {
      "path": "src/lib/tf.js",
      "globalName": "tf"
    },
    "tfvis": {
      "path": "src/lib/tfvis.js",
      "globalName": "tfvis"
    }
  },
  "localizedResources": {
    "KnnCommandSetStrings": "lib/extensions/knn/loc/{locale}.js"
  }
}